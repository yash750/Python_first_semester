   ],
   "source": [
    "l = [9,8,7,6,5,4,3,2,1]  # Take a sample list.\n",
    "l1 = []\n",
    "for i in l[::-1]:       # By negative indexing, take numbers in reverse order\n",
    "    l1.append(i)        # And appending them into another list\n",
    "print(l1)"
   ]
  }
